<%= simple_form_for [ @friend, @booking ] do |f| %>
  <%= f.input :start_date, as: :string, required: false, input_html: {class: "datepicker", value: params[:start_date]} %>
  <%= f.input :end_date, as: :string, required: false, input_html: {class: "datepicker", value: params[:end_date]} %>
  <%= f.submit "Book this friend" %>
<% end %>
